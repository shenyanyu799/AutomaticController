<Window x:Class="AutomaticController.Windows.Demos.吸尘器空气性能测试.StartWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AutomaticController.Windows.Demos.吸尘器空气性能测试"
        mc:Ignorable="d"
        Title="StartWindow" Height="450" Width="800" WindowStartupLocation="CenterScreen" ResizeMode="CanMinimize">
    <Grid HorizontalAlignment="Center" VerticalAlignment="Center">
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <TextBlock Text="吸尘器空气性能测试&#10;Air Performance Test System" FontSize="36" FontWeight="Bold" Margin="0,5,0,5" TextAlignment="Center"/>
        <Button x:Name="Button1" Grid.Row="1" Content="美标 American standard" FontSize="36"  Margin="0,5,0,5" Click="Button_Click"/>
        <Button x:Name="Button2" Grid.Row="2" Content="欧标 European standard" FontSize="36"  Margin="0,5,0,5" Click="Button_Click_1"/>
    </Grid>
</Window>
